<link rel="stylesheet" href="/css/contact.css">

<div class="container">
    <h1>Contact Us</h1>

    <div class="contact-info">
      <img src="/ccc.jpg" alt="Contact Person 1">
      <div>
        <h2>Contact Person 1</h2>
        <p>Email: contact1@example.com</p>
        <p>Phone: +1 555-1234</p>
      </div>
    </div>

    <div class="contact-info">
      <img src="contact2.jpg" alt="Contact Person 2">
      <div>
        <h2>Contact Person 2</h2>
        <p>Email: contact2@example.com</p>
        <p>Phone: +1 555-5678</p>
      </div>
    </div>

    <!-- Add more contact persons as needed -->

    <div class="contact-form">
      <form action="#" method="post">
        <label for="name">Your Name:</label>
        <input type="text" id="name" name="name" required>

        <label for="email">Your Email:</label>
        <input type="email" id="email" name="email" required>

        <label for="message">Your Message:</label>
        <textarea id="message" name="message" rows="4" required></textarea>

        <button type="submit">Submit</button>
      </form>
    </div>
  </div>
